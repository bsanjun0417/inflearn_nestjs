{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug NestJS",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:debug"
      ],
      "port": 9229,//nest.js기본 디버그 포트
      "restart": true,
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}
